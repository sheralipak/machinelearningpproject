from sklearn.linear_model import LinearRegression
from sklearn.metrics import mean_squared_error

# Load data
X = [[0], [1], [2], [3], [4], [5]]
y = [0, 1, 2, 3, 4, 5]

# Train model
model = LinearRegression()
model.fit(X, y)

# Predict values
y_pred = model.predict(X)

# Calculate RMSE
rmse = mean_squared_error(y, y_pred, squared=False)

print("RMSE:",Â rmse)
